{
  "name": "assasz/legends",
  "description": "API for RPG game",
  "type": "project",
  "license": "proprietary",
  "authors": [
    {
      "name": "Pawe≈Ç Antosiak",
      "email": "contact@pawelantosiak.com"
    }
  ],
  "require": {
    "php": "^8.4",
    "ext-mongodb": "*",
    "lambdish/phunctional": "^2.1",
    "mongodb/mongodb": "^2.1",
    "phpdocumentor/reflection-docblock": "^5.6",
    "phpstan/phpdoc-parser": "^2.1",
    "ramsey/uuid": "^4.8",
    "symfony/console": "^7.3",
    "symfony/dotenv": "^7.3",
    "symfony/event-dispatcher": "7.3.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "^7.3",
    "symfony/property-access": "7.2.*",
    "symfony/property-info": "7.2.*",
    "symfony/runtime": "^7.3",
    "symfony/serializer": "7.2.*",
    "symfony/yaml": "^7.3"
  },
  "require-dev": {
    "jetbrains/phpstorm-attributes": "^1.0",
    "phpstan/phpstan": "^0.12",
    "phpunit/phpunit": "^12",
    "symfony/browser-kit": "^7.3",
    "zoltanka/bypass-readonly": "^0.0.4"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.2.*"
    }
  },
  "autoload": {
    "psr-4": {
      "Legends\\Game\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Legends\\Game\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
